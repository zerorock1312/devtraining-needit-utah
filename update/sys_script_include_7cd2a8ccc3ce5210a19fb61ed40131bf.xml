        <name>Get Manager</name>
        <sandbox_callable>false</sandbox_callable>
        <script><![CDATA[var GetManager = Class.create();
GetManager.prototype = {
    initialize: function() {},

    getManager: function() {
		var referenceId = this.getParameter('sysparm_reference_id');
        if (!referenceId) {
            return ''; // Return an empty string if no referenceId is provided
        }

        var gr = new GlideRecord('sys_user_group'); // Query the sys_user_group table
        if (gr.get(referenceId)) {
            return gr.getValue('Manager'); // Return the manager field value
        }

        return ''; // Return an empty string if the record is not found
    },

    type: 'GetManager'
};]]></script>
